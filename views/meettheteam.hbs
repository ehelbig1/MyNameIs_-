{{> header}}

{{test}}

<div class="container">
<div class="row">
{{#each teamMembers}}
<div class="col-sm-5 col-md-4 col-lg-3">
    <div class="card" style="width: 18rem;">
        <img class="card-img-top" src={{#if this.headshot.url}}{{this.headshot.url}}{{else}}'#'{{/if}} alt={{this.headshot.alt}} width="340" height="340">
        <div class="card-body">
            <h5 class="card-title">{{this.firstName}} {{this.lastName}}</h5>
            <p class="card-text">{{#if this.jobTitle}}{{this.jobTitle}}{{else}}Unknown{{/if}}</p>
            <a href="/meettheteam/{{this.id}}" class="btn btn-primary">Learn More!</a>
        </div>
    </div>
</div>
{{/each}}
</div>
</div>

{{> footer}}